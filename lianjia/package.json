    "scripts": {
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 0.1,
                "functions": 0.1,
                "lines": 0.1,
                "statements": 0.1
            }
        },
        "coverageReporters": [
            "lcov"
        ],
        "coverageDirectory": "<rootDir>/coverage",
        "coveragePathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/src/index.js",
            "<rootDir>/src/http/",
            "<rootDir>/src/socket/",
            "<rootDir>/src/services/keyPressFilter/",
            "<rootDir>/src/components/DiskConfiguration/",
            "<rootDir>/src/components/FSTransfer/",
            "<rootDir>/src/views/ServiceAndClient/",
            "<rootDir>/src/views/Initialize/InitializeRaw.js",
            "<rootDir>/src/views/StoragePool/CreateTarget.js"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "<rootDir>/config/jest/setup.js",
            "jest-localstorage-mock"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|less|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^StyleSheets$": "<rootDir>/src/styleSheets/index.less",
            "^Images/(.*)$": "<rootDir>/src/images/$1",
            "^Components/(.*)$": "<rootDir>/src/components/$1",
            "^RouterPath$": "<rootDir>/src/routerPath/index.js",
            "^Views/(.*)$": "<rootDir>/src/views/$1",
            "^Redux/(.*)$": "<rootDir>/src/redux/$1",
            "^Store$": "<rootDir>/src/redux/index.js",
            "^Actions/(.*)$": "<rootDir>/src/redux/actions/$1",
            "^Reducers$": "<rootDir>/src/redux/reducers/index.js",
            "^Services$": "<rootDir>/src/services/index.js",
            "^Http/(.*)$": "<rootDir>/src/http/$1",
            "^Socket$": "<rootDir>/src/socket/index.js"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "proxy": "http://localhost:3000"
}
